{
  "git": {
    "deploymentEnabled": {
      "feature/dual-registration": true
    }
  },
  "github": {
    "autoAlias": true,
    "silent": false
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
